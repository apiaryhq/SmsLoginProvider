{
    "name": "apiaryhq/silex-sms-login-provider",
    "description": "SMS based login service for Silex",
    "keywords": ["apiary","silex","twilio"],
    "homepage": "https://github.com/apiaryhq/silex-sms-login-provider",
    "license": "MIT",
    "authors": [
        {
            "name": "Darren Mothersele",
            "email": "darren@apiaryhq.com",
            "homepage": "http://www.darrenmothersele.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "symfony/security": "^2.7",
        "twilio/sdk": "^4.6",
        "silex/silex": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "symfony/browser-kit": "^2.7",
        "monolog/monolog": "^1.17",
        "symfony/twig-bridge": "^2.7",
        "symfony/css-selector": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Apiary\\SmsLoginProvider\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apiary\\SmsLoginProvider\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
